# Gemini API Key (required)
GEMINI_API_KEY=your_gemini_api_key_here

# Webhook URL for fetching messages (optional)
# This is the n8n webhook URL that returns JSON with a "message" field
WEBHOOK_URL=https://your-n8n-webhook-url.com/webhook/your-webhook-id

# Webhook URL for Lynch stock score analysis (optional)
# This is the n8n webhook URL that accepts POST with {"token": "your_token", "tickers": ["TICKER"]}
WEBHOOK_LYNCH_STOCK=https://n8n.digitribe.se/webhook/ca4d7670-6bd3-4b84-ac23-3601cb85b3b0

# Authentication token for Lynch stock webhook (optional)
# This token is sent in the POST request body to authenticate with the webhook
WEBHOOK_LYNCH_TOKEN=your_secret_token_here

# Webhook URL for controlling garden lamps (optional)
# This is the n8n webhook URL that accepts POST with {"value": "on"} or {"value": "off"}
WEBHOOK_LAMP_GARDENPOST=https://your-n8n-webhook-url.com/webhook/your-lamp-webhook-id
